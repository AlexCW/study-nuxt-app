{
  "name": "study-app",
  "version": "1.0.0",
  "description": "Application to help document what I am studying",
  "author": "Alex Williams",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint",
    "test": "ava --serial --verbose",
    "test-watch": "ava --watch"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.0.0",
    "@nuxtjs/dotenv": "^1.3.0",
    "cross-env": "^5.2.0",
    "nuxt": "^2.0.0"
  },
  "devDependencies": {
    "nodemon": "^1.11.0",
    "babel-eslint": "^8.2.1",
    "eslint": "^5.0.1",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-vue": "^4.0.0",
    "ava": "^0.19.1",
    "jsdom": "^11.0.0"
  },
  "ava": {
    "files": [
			"tests/**/*.js"
    ],
    "sources": [
			"**/*.{js}"
    ],
    "compileEnhancements": false,
    "cache": true
  }
}
